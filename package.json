{
  "name": "skm-lit",
  "version": "0.0.14",
  "description": "skm-lit is a library for retrieving secrets stored in Cloud Storage",
  "author": "Joel Vo√ü <mail@joelvoss.com>",
  "license": "MIT",
  "homepage": "https://github.com/joelvoss/skm-lit",
  "bugs": {
    "url": "https://github.com/joelvoss/skm-lit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelvoss/skm-lit.git"
  },
  "main": "dist/cjs/index.cjs.js",
  "module": "dist/esm/index.es.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "jvdx build rollup 'src/index.ts' -f esm,cjs -c",
    "lint": "jvdx lint",
    "format": "jvdx format",
    "test": "jvdx test",
    "validate": "npm run format && npm run lint && npm run test",
    "clean": "jvdx clean dist",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "@google-cloud/storage": "4.7.0",
    "dotenv": "8.2.0",
    "mkdirp": "1.0.4",
    "read-pkg-up": "7.0.1",
    "tmp": "0.1.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/mkdirp": "1.0.0",
    "@types/tmp": "0.1.0",
    "jvdx": "1.1.1",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "jvdx pre-commit"
    }
  }
}
