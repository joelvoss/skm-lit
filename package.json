    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/runtime": "7.8.3",
    "@google-cloud/storage": "4.2.0",
    "dotenv": "8.2.0",
    "mkdirp": "0.5.1",
    "read-pkg-up": "7.0.1",
    "tmp": "0.1.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@types/jest": "24.9.0",
    "@types/mkdirp": "0.5.2",
    "@types/tmp": "0.1.0",
    "jvdx": "0.1.2",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "jvdx pre-commit"
    }
  }
