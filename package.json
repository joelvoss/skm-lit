{
  "name": "skm-lit",
  "version": "0.0.7",
  "description": "skm-lit is a library for retrieving secrets stored in Cloud Storage",
  "author": "Joel Vo√ü <mail@joelvoss.com>",
  "license": "MIT",
  "main": "dist/skm-lit.cjs.js",
  "module": "dist/skm-lit.esm.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "jvdx build rollup 'src/index.ts' -f esm,cjs -c",
